<?xml version="1.0" encoding="UTF-8"?>
<plugin>

    <!-- Main plugin class -->
    <class>com.android.remotemanager.plugin.DeviceControlPlugin</class>
    
    <!-- Plugin meta-data -->
    <name>Device Control</name>
    <description>Controls Android Devices to do any thing we predefined</description>
    <author>ZhiMo Software</author>
    <version>1.1.0</version>
    <date>12/1/2009</date>
    <minServerVersion>3.7.0</minServerVersion>
    <licenseType>internal</licenseType>
    <!-- UI extension -->
    <adminconsole> 
        <tab id="RemoteDeviceControl" name="RemoteDevice"  description="Click to manage..."> 
            <sidebar id="RemoteDeviceSideBar" name="My Plugin"> 
               <item id="my-plugin" name="My Plugin Admin"  description="Click to administer settings for my plugin" />
             </sidebar>         
        </tab>  
    </adminconsole>
    
</plugin>
